	<iframe id="myIframeBox" src="about:blank" width="100%" height="500" frameBorder="0"></iframe>
</div><script src="https://cdn.mediafine.co.kr/comment/comment.js"></script>
<script>
$comment.service_code		= 'news';								// 현재 이용 서비스 (중요!!)
$comment.parent_idxno		= '59476';								// 댓글 부모 게시물 번호
$comment.member_check		= 'GUEST';								// 회원여부
$comment.member_nd			= '';								// 회원여부
$comment.captcha_view		= 'Y';						// 캡챠 노출 (로그인 및 권한에 따른 노출 상태)
$comment.bbs_table			= undefined;		// 게시판 테이블
$comment.comment_orderby	= 'I',			// 댓글 정렬
$comment.comment_page		= 1,															// 댓글 페이징
$comment.comment_count		= 0,															// 댓글 총 갯수
$comment.commentConfigSet();																// 댓글 설정 불러오기 (중요!!)

$(document).ready(function(e){

	// 댓글 정렬 변경
	$('#comment').on('click', '.btn-comment-orderby', function(e){
		$comment.commentList('load', $comment.comment_page, $comment.comment_orderby=$(this).data('data'));	// 댓글 불러오기
//		$comment.moveScroll($comment.list_container);										// 자동 스크롤 이동
	});

	// 답글 정렬 변경
	$('#comment').on('click', '.btn-sub-comment-orderby', function(e){
		var reply_parent_idxno = $(this).data('idxno');
		$('#comment-item-'+reply_parent_idxno+'-sub .comment-sub-orderby').val($(this).data('orderby'));
		$comment.commentSubList(reply_parent_idxno, 'reload');
//		$comment.moveScroll($comment.list_container);										// 자동 스크롤 이동
	});

	// 댓글 더보기
	$('#comment').on('click', '.btn-comment-more', function(e){
		$comment.moveScroll($(this));														// 자동 스크롤 이동
		$comment.commentList('add', ++$comment.comment_page, $comment.comment_orderby);						// 댓글 추가하기
	});

	// 댓글 쓰기
	$('#comment').on('click', '.btn-comment-save', function(e){
		if( $comment.textLength($(this).closest('form').find('.comment-content')) ){		// 저장 이벤트 전 글자 수 체크 (스팸 방지)
			$comment.commentWrite($(this).closest('form'));									// 댓글 저장
		}
	});

	// 댓글 쓰기 상단 이동 버튼 처리
	$('#comment').on('click', '.btn-comment-write', function(e){
		$comment.moveScroll($('.comment-write'), true);										// 자동 스크롤 이동
		$('#comment > .comment-write .comment-content').focus();
	});

	// 댓글 작성 글자수 제한
	$('#comment').on('change keyup keydown keypress', '.comment-content', function(e){
		$comment.textLength($(this));
	});

	
	// 댓글 삭제 모달 열기
	$('#comment').on('click', '.btn-comment-delete', function(e){
		var cmtItem = $(this).closest('.user-textbox');

		$comment.reveal_delete.find('.mode').val($(this).data('mode'));
		$comment.reveal_delete.find('.idxno').val(cmtItem.data('idxno'));
		$comment.reveal_delete.find('.reply-parent-idxno').val(cmtItem.data('reply-parent-idxno'));
		$comment.reveal_delete.find('.user-id').val(cmtItem.data('user-id'));
		$comment.reveal_delete.find('.password').val('');

		$comment.reveal_delete.foundation('open');
	});

	// 댓글 삭제 시 엔터 이벤트
	$('#comment-reveal-delete .password').keydown(function(e) {
		if (e.keyCode == 13) return false;
	});

	// 댓글 삭제
	$('#comment-reveal-delete').on('click', '.btn-comment-delete-ok', function(e){
		$comment.commentDelete($(this).closest('form'));									// 댓글 삭제
	});

	// 댓글 답글 보기 / 더보기
	$('#comment').on('click', '.btn-comment-sub', function(e){
		var sub_item = $(this).closest('.comment-item'),
			reply_parent_idxno = sub_item.data('reply-parent-idxno'),
			sub_mode = $(this).data('sub-mode');

		$comment.commentSubList(reply_parent_idxno, sub_mode);					// 답글 보기
	});

	// 답글 쓰기 폼 열기
	$('#comment').on('click', '.btn-comment-sub-write', function(e){
		var sub_item = $(this).closest('.comment-item'),
			sub_write_form = sub_item.find('form[name="commentWriteForm"]');

		$(this).hide();
		sub_write_form.show().find('textarea.comment-content').focus();

		$comment.moveScroll(sub_write_form);												// 자동 스크롤 이동
	});

	// 댓글 추천/반대
	$('#comment').on('click', '.btn-comment-vote', function(e){
		var cmtItem = $(this).closest('.user-vote');
		$comment.commentVote($(this), $(this).data('mode'), cmtItem.data('idxno'));			// 댓글 추천/반대
	});

	// 댓글 작성란 비활성 이벤트
	$('#comment').on('click', '.comment-write .comment-content', function(e){
		if( $(this).prop('readonly') == true ){
			var login_url = $(this).closest('.comment-write').find('.btn-comment-login').prop('href');

			if($comment.member_check == 'GUEST') location.href=login_url;
			else alert('댓글 작성 권한이 없습니다. 관리자에게 문의하세요');
		}
	});

	// 댓글 옵션 레이어 토글
	$('#comment').on('click', '.user-tb-h-del button', function(){
		var optionBox = $(this).closest('.user-tb-h-del').find('.user-delete');
		if(optionBox.is(':visible') == false){
			$('.user-delete').hide();
			optionBox.show();
		} else optionBox.hide();
	});

	// 베스트 댓글 안내
	$('.cl-s-explain a').on('mouseover', function(){
		$(this).next('p').show();
	}).on('mouseout', function(){
		$(this).next('p').hide();
	});

		// 댓글 작성자 정보 OPEN
	$('#comment').on('click', '.user-comment-info', function(){

		var infoId = $(this).closest('.comment-item').attr('id');
		$comment.userCommentInfo($(this).closest('.user-textbox').data('user-id'));

		$('html, body').addClass('block');

		// 댓글 작성자 정보 CLOSE
		$(document).on('click', '.cmt-info-block, .cmt-info-close', function(e){

			var $tgPoint		= $(e.target);
			var $commentArea	= $tgPoint.closest('.cmt-info'); // .hasClass('cmt-info');
			var closeBool		= true;

			if($commentArea.length) {
				if(!$tgPoint.hasClass('icon-close-thin')) closeBool = false;
			}

			if(closeBool) {
				$('html, body').removeClass('block');
				$('#userCommentInfo').remove();
			}
//			location.href='#'+infoId;
		});
	});
	
	$('#comment').on('click', '.my-cmt', function(e){
		$('#myIframeBox').attr("src", "/comment/myComment.html");
		$('#myCommnetView').foundation('open');
	});
});
</script>

